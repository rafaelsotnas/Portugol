programa
{
	
	funcao inicio()
	{
		real nota1,nota2,nota3,media
		escreva("Valor nota1: ")
		leia(nota1)
		escreva("Valor nota2: ")
		leia(nota2)
		escreva("Valor nota2: ")
		leia(nota3)
		media=(nota1*2 + nota2*3 + nota3*5)/10
		escreva("Sua nota média é: ",media)
	}
}
